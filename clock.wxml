<!--pages/clock/clock.wxml-->
<view class="bg">
  <view class="content">
    <image src="/pages/images/ditu.jpg" class="photo"></image>  
    <button type="primary" class="loginBtn" bindtap="jumpPage" style="width:160px;height:40px;margin-left:70px;margin-top:50px;margin-bottom:50px" bindtap="jumpClock" bindtap="btnTap">提交今日打卡</button>
    <view>
      <modal title="打卡成功" hidden="{<!-- -->{modalHidden}}" confirm-text="确定" bindconfirm="changeModel" bindcancel="modelCancel"> 
      </modal>
    </view>
  </view>
</view>
